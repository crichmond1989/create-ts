{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules"
    ]
  },
  "jsRules": true,
  "rules": {
    "import-name": false,
    "max-line-length": [
      true,
      180
    ],
    "object-shorthand-properties-first": false,
    "quotemark": [
      true,
      "double",
      "backtick"
    ],
    "ter-arrow-parens": false,
    "ter-indent": false,
    "variable-name": [
      true,
      "allow-pascal-case"
    ]
  },
  "rulesDirectory": []
}